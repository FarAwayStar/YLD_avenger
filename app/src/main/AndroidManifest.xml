<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a10261.yld_avenger">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/yld_app_news2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/yld_app_news2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.yld_WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.yld_MainActivity"/>
        <activity android:name=".Activity.WebActivity" android:parentActivityName=".Activity.yld_MainActivity"/>
    </application>

</manifest>